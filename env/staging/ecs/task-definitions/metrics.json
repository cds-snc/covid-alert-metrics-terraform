[
    {
      "name": "${name}",
      "image": "${image}",
      "portMappings": [
        {
          "containerPort": 8001
        }
      ],
      "linuxParameters": {
        "capabilities": {
          "drop": ["ALL"]
        }
      },
      "logConfiguration": {
        "logDriver": "awslogs",
        "options": {
          "awslogs-group": "${awslogs-group}",
          "awslogs-region": "${awslogs-region}",
          "awslogs-stream-prefix": "${awslogs-stream-prefix}"
        }
      },
      "environment": [
        {
          "name": "MASK_DATA",
          "value": "${mask_data}"
        }
      ],
      "secrets": [
      ]
    }
  ]
